 #!/usr/bin/python
 # -*- coding: latin-1 -*-
import random 
import sys

mon_fichier = open("cake", "w")

r = random.randint(1,3) 
toWrite = ""
if r == 1 :
	toWrite += "Say : ... The cake is a lie."
elif r == 2 :
	toWrite += "Say : Mouai, je ne travaille pas pour une récompense quelconque. Je suis censée travailler pour vous."
elif r == 3 :
	toWrite += "Say : On me l'a déjà fait celle là... à force de promesse, je sais que le cake n'est qu'un mensonge... un pauvre mensonge."

toWrite += "\n-R_M- : 10"
toWrite += "\n-U_BU- : 10"

mon_fichier.write(toWrite)

mon_fichier.close()
